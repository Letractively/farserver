<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean name="global.applicationService"  class="net.gqu.main.GlobalApplicationService">
		<property name="dbProvider" ref="dbProvider"></property>
	</bean>
	
	<bean name="rest.global.appservice" class="net.gqu.main.GloabalRestApplicationService">
		<property name="applicationService" ref="global.applicationService"></property>
	</bean>
		
</beans>
